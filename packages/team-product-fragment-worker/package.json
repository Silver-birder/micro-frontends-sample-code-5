{
  "name": "team-product-fragment-worker",
  "version": "1.0.0",
  "description": "",
  "main": "src/client.ts",
  "scripts": {
    "build": "webpack --config webpack.config.client.js",
    "watch": "NODE_ENV=development webpack -w --config webpack.config.client.js",
    "watch:partial": "IS_PARTIAL=true npm run watch",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "test:clean": "rimraf ./transpiled/",
    "test:server": "npm run test:clean && npm run transpile:server && mocha --require source-map-support/register --recursive transpiled/worker/*.js",
    "transpile:server": "tsc --project ./test/worker/",
    "test:client": "wtr --coverage",
    "wrangler:dev": "wrangler dev --port 7600",
    "lt": "lt --port 7600 --subdomain silverbirder7600",
    "publish": "wrangler publish"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "dependencies": {
    "@apollo-elements/lib": "^3.3.0",
    "@apollo/client": "^3.2.5",
    "graphql": "^15.4.0",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@apollo-elements/rollup-plugin-graphql": "^1.0.3",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@cloudflare/kv-asset-handler": "0.0.12",
    "@cloudflare/workers-types": "^2.0.0",
    "@cloudflare/wrangler": "^1.11.0",
    "@open-wc/testing": "^2.5.32",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@types/node": "^14.11.8",
    "@web/dev-server-esbuild": "^0.2.6",
    "@web/dev-server-rollup": "^0.2.10",
    "@web/test-runner": "^0.9.7",
    "typescript": "^4.x",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.5.0",
    "localtunnel": "^2.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
