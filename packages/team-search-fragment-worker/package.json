{
  "name": "team-search-fragment-worker",
  "version": "1.0.0",
  "description": "",
  "main": "src/client.ts",
  "scripts": {
    "build": "webpack --config webpack.config.client.js",
    "watch": "NODE_ENV=development webpack -w --config webpack.config.client.js",
    "watch:partial": "IS_PARTIAL=true npm run watch",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "test:clean": "rimraf ./transpiled/",
    "test:server": "npm run test:clean && npm run transpile:server && mocha --require source-map-support/register --recursive transpiled/worker/*.js",
    "transpile:server": "tsc --project ./test/worker/",
    "test:client": "rimraf ./transpiled/client && npm run transpile:client && karma start karma.conf.cjs",
    "transpile:client": "tsc --project ./test/client/",
    "wrangler:dev": "wrangler dev --port 7300",
    "lt": "lt --port 7300 --subdomain silverbirder7300",
    "publish": "wrangler publish"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@cloudflare/kv-asset-handler": "0.0.12",
    "@cloudflare/workers-types": "^2.0.0",
    "@cloudflare/wrangler": "^1.11.0",
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.11",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.11.8",
    "@types/service-worker-mock": "^2.0.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.5.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "localtunnel": "^2.0.0",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
